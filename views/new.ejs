<%- include ('partials/header'); %>

    <div class="container">
        <div class="row">
            <h1 style="text-align: center;">Create Account</h1>
            <div style="width: 30%; margin: 30px auto;">
                <form action="/alumni" method="POST">
                    <div class="form-group">
                        <input class="form-control" type="text" name="name" placeholder="name" required>
                    </div>
                    <!-- <div class="form-group">
                        <input class="form-control" type="text" name="image" placeholder="image url">
                    </div> -->
                     
                    <div class="form-group">
                        <input class="form-control" type="text" name="college" placeholder="college" required>
                    </div>

                    <div class="form-group">
                        <input class="form-control" type="text" name="branch" placeholder="branch" required>
                    </div>

                    <div class="form-group">
                        <input class="form-control" type="text" name="mobile" placeholder="Enter mobile number with country code..+91" required>
                    </div>
                    <div class="form-group">
                        <input class="form-control" type="email" name="email" placeholder="email" required>
                    </div>

                    <div class="form-group">
                        <input class="form-control" type="text" name="batch" placeholder="batch" required>
                    </div>

                    <div class="form-group">
                        <input class="form-control" type="text" name="address" placeholder="address" required>
                    </div>

                    <div class="form-group">
                        <button class="btn btn-lg btn-primary btn-block">Submit!</button>
                    </div>


                </form>
                <a href="/alumni">Go Back</a>
            </div>

        </div>
    </div>
    <%- include ('partials/footer'); %>